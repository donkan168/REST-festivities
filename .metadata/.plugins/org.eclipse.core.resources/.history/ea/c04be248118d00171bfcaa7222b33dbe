/**
 * 
 */
package com.festivities.util;

 
public class Link {
	
	/** Atributos **/

	private String id;
	private String href;
	
	/** Constructores **/

	public Link(String id, String href) {
		super();
		this.id = id;
		this.href = href;
	}
	
	/**
	 * @return the id
	 */
	public String getId() {
		return id;
	}
	/**
	 * @param rel the rel to set
	 */
	public void setId(String id) {
		this.id = id;
	}
	/**
	 * @return the href
	 */
	public String getHref() {
		return href;
	}
	/**
	 * @param href the href to set
	 */
	public void setHref(String href) {
		this.href = href;
	}

}

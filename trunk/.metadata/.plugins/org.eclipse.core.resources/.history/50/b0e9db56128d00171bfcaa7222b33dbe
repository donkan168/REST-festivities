package com.grupoaval.adminBusiness.rest.schema;

/**
 * 
 * <p>Description: Representa las acciones que se pueden hacer sobre un estado
 * de un recurso. Se usa para implementar 'Hypermedia as the engine of
 * application state'. </p>
 * <b>IBM. Global Bussiness Services Colombia.</b>
 * <Grupo Aval (Bogota Colombia).<
 * <Vicepresidencia de operaciones.<
 *
 * @author <A HREF="mailto:ombohorq@co.ibm.com">Oscar Bohorquez</A> <p>
 * <b>Fecha de creaci√≥n(dd/mmm/aaaa): </b> [Dec 7, 2016]
 *
 * @version [1.0, Dec 7, 2016]
 *
 */
public class Link {
	
	/** Atributos **/

	private String id;
	private String href;
	
	/** Constructores **/

	public Link(String id, String href) {
		super();
		this.id = id;
		this.href = href;
	}
	
	/**
	 * @return the id
	 */
	public String getId() {
		return id;
	}
	/**
	 * @param rel the rel to set
	 */
	public void setId(String id) {
		this.id = id;
	}
	/**
	 * @return the href
	 */
	public String getHref() {
		return href;
	}
	/**
	 * @param href the href to set
	 */
	public void setHref(String href) {
		this.href = href;
	}

}
